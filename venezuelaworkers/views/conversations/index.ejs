<%- include('../top') %>
<div class="container">
    <div class="row">
        <div class="col">
            <div class="card mt-5">
                <div class="card-header">
                <h3>Project conversations</h3>
                </div>
                <div class="card-body">
                    <table class="table table-striped">
                        <tbody>
                            <tbody>
                                <% if(conversations) { %>
                                    <% conversations.forEach(function(conversation) {  %>
                                        <tr>
                                            <td><a href="/conversations/<%= conversation.id  %>"><%= conversation.title  %></a></td>
                                        </tr>
                                    <% }) %>
                                <% } %>
                            </tbody>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<%- include('../bottom') %>
